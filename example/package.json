{
  "name": "grunt-init-react",
  "description": "grunt-init template for a very basic react project.",
  "version": "0.1.7",
  "homepage": "https://github.com/skratchdot/grunt-init-react/",
  "author": {
    "name": "skratchdot",
    "url": "http://skratchdot.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skratchdot/grunt-init-react.git"
  },
  "bugs": {
    "url": "https://github.com/skratchdot/grunt-init-react/issues"
  },
  "engines": {
    "node": ">= 4.x.0"
  },
  "scripts": {
    "build:dev": "cross-env NODE_ENV=development webpack",
    "build:prod": "cross-env NODE_ENV=production webpack",
    "clean": "rimraf build/dev/* build/prod/* coverage/*",
    "cover": "cross-env NODE_ENV=test babel-node node_modules/isparta/bin/isparta cover node_modules/mocha/bin/_mocha -- --recursive test",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "lint": "eslint src test webpack.config.js",
    "test": "cross-env NODE_ENV=test mocha -r babel-register --recursive test",
    "server": "cross-env NODE_ENV=development webpack-dev-server",
    "watch": "npm-run-all clean --parallel watch:cover watch:lint server",
    "watch:cover": "npm run cover -- -w",
    "watch:lint": "watch 'npm run lint' .",
    "watch:test": "npm run test -- -w"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "babel-root-import": "^4.0.1",
    "bootstrap": "^3.3.6",
    "brfs": "^1.4.3",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^3.0.1",
    "coveralls": "^2.11.9",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "enzyme": "^2.3.0",
    "eslint": "^3.0.1",
    "eslint-plugin-react": "^5.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "history": "^3.0.0",
    "html-loader": "^0.4.3",
    "immutable": "^3.8.1",
    "isparta": "^4.0.0",
    "jsdom": "^9.4.1",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "markdown-loader": "^0.1.7",
    "material-ui": "^0.15.1",
