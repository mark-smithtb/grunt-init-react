  "scripts": {
    "build": "npm-run-all clean build:dev build:prod",
    "build:dev": "cross-env NODE_ENV=development webpack",
    "build:prod": "cross-env NODE_ENV=production webpack",
    "clean": "rimraf build/dev/* build/prod/* .nyc_output/* coverage/*",
    "cover": "cross-env NODE_ENV=test nyc npm test",
    "coveralls": "cat coverage/lcov.info | coveralls --verbose",
    "lint": "eslint src test webpack.config.js",
    "test": "cross-env NODE_ENV=test mocha -r babel-register --recursive test",
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "watch": "npm-run-all clean --parallel watch:test watch:lint start",
    "watch:lint": "watch 'npm run lint' .",
    "watch:test": "npm run test -- -w"
